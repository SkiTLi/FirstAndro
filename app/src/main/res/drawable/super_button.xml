<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:width="100dp" android:height="100dp" android:gravity="center">
        <shape android:shape="oval">
            <solid android:color="#d4aae6" />
            <corners android:radius="100dp" />

            <stroke
                android:width="5dp"
                android:color="#58c258" />
            <size android:width="100dp" android:height="100dp"/>


            <!--<size android:width="200dp" android:height="200dp"/>-->

        </shape>
    </item>
    <item android:width="70dp" android:height="70dp" android:gravity="center">

        <shape android:shape="oval">
            <solid android:color="#905da6" />
            <corners android:radius="100dp" />
            <stroke
                android:width="5dp"
                android:color="#e4cc15" />
            <!--<size android:width="100dp" android:height="100dp"/>-->

        </shape>
    </item>



</layer-list>